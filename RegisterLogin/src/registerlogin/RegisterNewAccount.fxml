         </font>
      </Label>
      <PasswordField fx:id="pass" layoutX="77.0" layoutY="174.0" prefHeight="37.0" prefWidth="181.0" promptText="Password" />
      <Button defaultButton="true" layoutX="75.0" layoutY="231.0" mnemonicParsing="false" onAction="#login" prefHeight="25.0" prefWidth="119.0" text="Enter">
         <font>
            <Font size="16.0" />
         </font>
      </Button>
      <Button layoutX="75.0" layoutY="278.0" mnemonicParsing="false" onAction="#newuser" prefHeight="37.0" prefWidth="167.0" text="Register New User.">
