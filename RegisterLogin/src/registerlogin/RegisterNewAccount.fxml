<AnchorPane id="AnchorPane" prefHeight="348.0" prefWidth="499.0" xmlns="http://javafx.com/javafx/8.0.60" xmlns:fx="http://javafx.com/fxml/1" fx:controller="registerlogin.RegisterNewAccountController">
